  {% if youAreHere %}
    <p class="logo__you-are-here"><span class="visually-hidden">U bent nu hier: </span>
    {% for here in youAreHere %}
      <span>{{ here.text }}</span>
    {% endfor %}
    </p>
  {% endif %}
</div>
